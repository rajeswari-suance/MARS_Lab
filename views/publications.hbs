  <body>

    <!--Content Part ==================================================
================================================== -->
    <div id="header">
      <div class="container">
        <!-- Header | Logo, Menu================================================== -->
        <div class="logo">
          <a href="index.html"><img
              class="imageLogo"
              src="images/logo_2.jpeg"
              alt="Logo"
            /></a>
        </div>
        <div class="mainmenu">
          <div id="mainmenu">
            <ul class="sf-menu">
              <li><a href="index.html">Home</a></li>
              <li><a href="people.html">People<img
                    src="images/icons8-double-down-24.png"
                    alt="Drop Down"
                    width="20"
                  /></a>
                <ul>
                  <li><a href="alumini.html">MARS Alumni</a></li>
                </ul>
              </li>
              <li><a href="infrastructure.html">Infrastructure</a></li>
              <li><a href="research.html">Research</a></li>
              <li><a href="publications.html" id="visited">Publications</a></li>
              <li><a href="news.html">News</a></li>
              <li><a href="gallery.html">Gallery</a></li>
              <li><a
                  href="https://www.youtube.com/watch?v=MibSikAjAM0"
                  target="_blank"
                >Video</a></li>
            </ul>
          </div>
          <!-- mainmenu ends here -->

          <!-- Responsive Menu -->
          <form id="responsive-menu" action="#" method="post">
            <div id="overlay">
              <a href="index.html" id="visited">Home</a>
              <a href="people.html">People</a>
              <a href="alumini.html">MARS Alumni</a>
              <a href="infrastructure.html">Infrastructure</a>
              <a href="research.html">Research</a>
              <a href="publications.html">Publications</a>
              <a href="news.html">News</a>
              <a href="gallery.html">Gallery</a>
              <a href="https://www.youtube.com/watch?v=MibSikAjAM0">Video</a>
            </div>
            <div class="nav">
              <div id="hamburger">
                <div></div>
              </div>
            </div>
          </form>
        </div>
        <!-- mainmenu ends here -->
      </div>
      <!-- container ends here -->
    </div>
    <!-- header ends here -->
    <!--Breadcrumbs ==================================================
================================================== -->
    <div class="breadcrumbs">
      <div class="container">
        <header>
          <marquee>
            <a href="https://www.pmrf.in/">
              <img src="images/new-2.gif" height="20px" width="35px" />
              Publications Dynamics
            </a>
          </marquee>
        </header>
      </div>
      <!-- container ends here -->
      <hr class="separator1" />
    </div>
    <!-- breadcrumbs ends here -->
    <div class="wrapper">
      <div class="container contact">
        <!-- Contact Sidebar ==================================================
    ================================================== -->

        <div class="one-half">
          <h2 class="center">Publications _ updated123</h2>
          <div class="buttons">
            <button id="book-chapter">Book Chapter</button>
            <button id="journal-article">Journal Publications</button>
            <button id="conference-papers">Conference Publications</button>
          </div>

          <div id="book-chapter-view" style="display:none;">
            Book Chapter
          </div>

          <div id="journal-article-view" style="display:none;">
            Journal
          </div>
          <div id="conference-papers-view" style="display:none;">
            Conference
          </div>

          <script>
            const bookChapterBtn = document.querySelector('#book-chapter');
            const journalArticleBtn =
            document.querySelector('#journal-article'); const conferencePaperBtn
            = document.querySelector('#conference-papers'); const
            bookChapterView = document.querySelector('#book-chapter-view');
            const journalArticleView =
            document.querySelector('#journal-article-view'); const
            conferencePaperView =
            document.querySelector('#conference-papers-view');
            journalArticleView.style.display = 'block';
            bookChapterBtn.addEventListener('click', () => {
            bookChapterView.style.display = 'block';
            journalArticleView.style.display = 'none';
            conferencePaperView.style.display = 'none'; });
            journalArticleBtn.addEventListener('click', () => {
            bookChapterView.style.display = 'none';
            journalArticleView.style.display = 'block';
            conferencePaperView.style.display = 'none'; });
            conferencePaperBtn.addEventListener('click', () => {
            bookChapterView.style.display = 'none';
            journalArticleView.style.display = 'none';
            conferencePaperView.style.display = 'block'; });
          </script>

          <hr class="separator1" />
          {{#each publications}}

            <p></p>
            <!-- etc. -->

            <section class="first shadow">
              <ul>
                <li>
                  <a href="{{this.doi}}" target="_blank">
                    <h3>"{{this.title}}"</h3>
                  </a>
                </li>
                <li>
                  <h5>{{this.author}}</h5>
                </li>
                <li>(Accepted in) {{this.journal}}</li>
              </ul>
            </section>
          {{/each}}
        </div>
      </div>
      <div class="blankSeparator"></div>
      <hr class="separator2" />
      <!-- Copyright ==================================================================================================== -->
      <div id="copyright">
        <div class="container">
          <p class="copyright">&copy; Copyright 2018. &quot;MARS Lab Indian
            Institute of Technology Guwahati</p>
        </div>
        <!-- container ends here -->
      </div>
    </div>
    <!-- End Document
================================================== -->
    <!-- Scripts ==================================================
================================================== -->
    <script src="js/jquery-1.8.0.min.js" type="text/javascript"></script>
    <!-- Main js files -->
    <script src="js/screen.js" type="text/javascript"></script>
    <!-- Tabs -->
    <script src="js/tabs.js" type="text/javascript"></script>
    <!-- Include prettyPhoto -->
    <script src="js/jquery.prettyPhoto.js" type="text/javascript"></script>
    <!-- Include Superfish -->
    <script src="js/superfish.js" type="text/javascript"></script>
    <script src="js/hoverIntent.js" type="text/javascript"></script>
    <!-- Flexslider -->
    <script src="js/jquery.flexslider-min.js" type="text/javascript"></script>
    <!-- Modernizr -->
    <script type="text/javascript" src="js/modernizr.custom.29473.js"></script>
  </body>

</html>